-content: center;\" >\n        <div style=\"text-align: center; font-size: 30px; margin-bottom: 50px; color: #333 ;font-size: 30px;color: #000000\" >宠 物 领 养 系 统</div>\n          <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\">\n            <el-form-item prop=\"username\">\n              <el-input size=\"medium\" prefix-icon=\"el-icon-user\" v-model=\"user.username\" placeholder=\"请输入账号\"></el-input>\n            </el-form-item>\n            <el-form-item prop=\"password\" style=\"margin-bottom: 50px\">\n              <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" show-password v-model=\"user.password\" placeholder=\"请输入密码\"></el-input>\n            </el-form-item>\n            <el-form-item style=\"margin-top: 30px; display: flex;\">\n              <el-button style=\"flex: 1;width: 100px\" type=\"primary\" size=\"small\"  autocomplete=\"off\" @click=\"login\">登录</el-button>\n              <el-button style=\"flex: 1 ;width: 100px\" type=\"warning\" size=\"small\"  autocomplete=\"off\" @click=\"$router.push('/register')\">前往注册</el-button>\n              <el-button style=\" flex: 1 ;width: 100px;background-color: #67C23A;color: #ffffff\" size=\"small\" autocomplete=\"off\" @click=\"handlePass\">找回密码</el-button>\n            </el-form-item>\n          </el-form>\n        </div>\n\n        <el-dialog title=\"找回密码\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\n          <el-form label-width=\"100px\" size=\"small\">\n            <el-form-item label=\"用户名\">\n              <el-input v-model=\"pass.username\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"手机号\">\n              <el-input v-model=\"pass.phone\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"passwordBack\">重置密码</el-button>\n          </div>\n        </el-dialog>\n\n\n   </div>\n\n\n  </div>\n</template>\n\n<script>\nimport {resetRouter, setRoutes} from \"@/router\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      user: {},\n      pass: {},\n      dialogFormVisible: false,\n      rules: {\n        username: [\n          { required: true, message: '请输入用户名', trigger: 'blur' },\n          { min: 3, max: 10, message: '长度在 3 到 5 个字符', trigger: 'blur' }\n        ],\n        password: [\n          { required: true, message: '请输入密码', trigger: 'blur' },\n          { min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur' }\n        ],\n      }\n    }\n  },\n  created() {\n    // 重置路由\n    resetRouter()\n  },\n  methods: {\n    login() {\n      this.$refs['userForm'].validate((valid) => {\n        if (valid) {  // 表单校验合法\n          this.request.post(\"/user/login\", this.user).then(res => {\n            if(res.code === '200') {\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // 存储用户信息到浏览器\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // 存储用户信息到浏览器\n\n              // 动态设置当前用户的路由\n              setRoutes()\n              this.$router.push(\"/front/home\")\n              this.$message.success(\"登录成功\")\n            } else {\n              this.$message.error(res.msg)\n            }\n          })\n        }\n      });\n    },\n    handlePass() {\n      this.dialogFormVisible = true\n      this.pass = {}\n    },\n    passwordBack() {\n      this.request.put(\"/user/reset\", this.pass).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"重置密码成功，新密码为：123，请尽快修改密码\")\n          this.dialogFormVisible = false\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style>\n.container {\n  height: 100vh;\n  overflow: hidden;\n  /*background-color: #dcd5b0;*/\n  background-image: url(\"../assets/bg2.png\");\n  background-size: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  color: #666;\n}\n.wrapper {\n  height: 100vh;\n  background-image: linear-gradient(to bottom right, #4169E1 , \t#87CEFA);\n  overflow: hidden;\n}\n</style>\n"],"mappings":";AA6CA,SAAAA,WAAA,EAAAC,SAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA,EACA;QACAG,QAAA,GACA;UAAAL,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAI,QAAA;IACA;IACAf,WAAA;EACA;EACAgB,OAAA;IACAC,MAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UAAA;UACA,KAAAC,OAAA,CAAAC,IAAA,qBAAAlB,IAAA,EAAAmB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACAC,YAAA,CAAAC,OAAA,SAAAC,IAAA,CAAAC,SAAA,CAAAL,GAAA,CAAArB,IAAA;cACAuB,YAAA,CAAAC,OAAA,UAAAC,IAAA,CAAAC,SAAA,CAAAL,GAAA,CAAArB,IAAA,CAAA2B,KAAA;;cAEA;cACA7B,SAAA;cACA,KAAA8B,OAAA,CAAAC,IAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;YACA;cACA,KAAAD,QAAA,CAAAE,KAAA,CAAAX,GAAA,CAAAY,GAAA;YACA;UACA;QACA;MACA;IACA;IACAC,WAAA;MACA,KAAA/B,iBAAA;MACA,KAAAD,IAAA;IACA;IACAiC,aAAA;MACA,KAAAjB,OAAA,CAAAkB,GAAA,qBAAAlC,IAAA,EAAAkB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAQ,QAAA,CAAAC,OAAA;UACA,KAAA5B,iBAAA;QACA;UACA,KAAA2B,QAAA,CAAAE,KAAA,CAAAX,GAAA,CAAAY,GAAA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]}]}